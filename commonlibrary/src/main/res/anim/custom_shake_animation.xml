<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:repeatCount="10"
    android:interpolator="@anim/custom_cycleinterpolator">
    <translate
        android:fromXDelta="0"
        android:toXDelta="-10"
        android:fromYDelta="0"
        android:toYDelta="-10"
        android:duration="300"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="10"
        android:fromYDelta="0"
        android:toYDelta="-10"
        android:startOffset="300"
        android:duration="300"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="-10"
        android:fromYDelta="0"
        android:toYDelta="10"
        android:startOffset="600"
        android:duration="300"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="10"
        android:fromYDelta="0"
        android:toYDelta="10"
        android:startOffset="900"
        android:duration="300"/>
</set>